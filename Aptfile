# Use Ubuntu Bionic package sources
RUN echo "deb http://archive.ubuntu.com/ubuntu bionic main" >> /etc/apt/sources.list
RUN echo "deb http://archive.ubuntu.com/ubuntu bionic-updates main" >> /etc/apt/sources.list

# Update package list
RUN apt-get update

# Install required packages
RUN apt-get install -y libatk1.0-0

#libatk1.0-0
libatk-bridge2.0-0
libdrm2
libxkbcommon0
libxcomposite1
libxdamage1
libxfixes3
libxrandr2
libgbm1
libasound2